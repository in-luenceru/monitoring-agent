# Ignore log files
logs/*
logs/ossec.log*
logs/wazuh-*.log*

# Ignore runtime files and queue databases
var/run/*
var/lib/*
queue/**
queue/rids/*
queue/agents/*
queue/diff/*
queue/ossec/*
queue/fim/**
queue/logcollector/**
queue/sockets/**
queue/syscollector/**

# Ignore backup files
etc/ossec.conf.backup.*
etc/*.save

# Ignore database files
backup/db/*.gz

# Ignore temporary files
tmp/*
.setup_complete

# Keep directory structure but ignore contents
!logs/.gitkeep
!var/run/.gitkeep
!queue/rids/.gitkeep
# Ensure client keys file isn't committed
etc/client.keys
